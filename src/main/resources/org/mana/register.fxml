<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
            <Label minWidth="630.0" styleClass="header" text="Customer Register">
                <font>
                    <Font name="System Bold" size="13.0" />
                </font>
            </Label>
      <VBox spacing="25.0">
         <children>
                  <HBox fillHeight="false" spacing="30.0">
                      <children>
                          <VBox>
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" styleClass="item-title" text="First Name" />
                        <TextField fx:id="firstnameField" maxWidth="300.0" minWidth="300.0" />
                              </children>
                          </VBox>
                          <VBox>
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" prefWidth="300.0" styleClass="item-title" text="Last Name" />
                                  <TextField fx:id="lastnameField" maxWidth="300.0" minWidth="300.0" />
                              </children>
                              <HBox.margin>
                                  <Insets />
                              </HBox.margin>
                          </VBox>
                      </children>
                  </HBox>
                  <HBox fillHeight="false" maxWidth="630.0" minWidth="630.0" prefWidth="630.0" spacing="30.0">
                      <children>
                          <VBox>
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" styleClass="item-title" text="Date of Birth" />
                                  <DatePicker fx:id="dobPicker" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" promptText="please, click the button -&gt;" />
                              </children>
                          </VBox>
                          <VBox>
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" styleClass="item-title" text="City" />
                                  <TextField fx:id="cityField" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
                              </children>
                              <HBox.margin>
                                  <Insets />
                              </HBox.margin>
                          </VBox>
                      </children>
                  </HBox>
                  <HBox maxWidth="630.0" minWidth="630.0" prefWidth="630.0" spacing="30.0">
                      <children>
                          <VBox>
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" styleClass="item-title" text="Username" />
                                  <TextField fx:id="usernameField" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
                              </children>
                          </VBox>
                          <VBox fillWidth="false">
                              <children>
                                  <Label alignment="BOTTOM_LEFT" maxWidth="300.0" styleClass="item-title" text="Password" />
                                  <PasswordField fx:id="passwordField" maxWidth="300.0" minWidth="300.0" />
                              </children>
                              <HBox.margin>
                                  <Insets />
                              </HBox.margin>
                          </VBox>
                      </children>
                  </HBox>
            <HBox prefWidth="200.0">
               <children>
                  <VBox>
                     <children>
                        <Label text="Role" />
                        <ChoiceBox fx:id="roleChoice" prefWidth="300.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
                  <HBox alignment="CENTER_RIGHT" maxWidth="600.0" minWidth="630.0" prefWidth="600.0" spacing="30.0">
                      <children>
                          <Button fx:id="secondaryButton" cancelButton="true" onAction="#goBack" text="Cancel">
                              <HBox.margin>
                                  <Insets />
                              </HBox.margin>
                              <padding>
                                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                              </padding>
                          </Button>
                          <Button defaultButton="true" mnemonicParsing="false" onAction="#register" text="Register">
                              <HBox.margin>
                                  <Insets />
                              </HBox.margin>
                              <padding>
                                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                              </padding>
                          </Button>
                      </children>
                      </HBox>
         </children>
      </VBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>

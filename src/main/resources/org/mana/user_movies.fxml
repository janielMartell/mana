<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="687.0" prefWidth="685.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.mana.controller.UserMoviesController">
   <content>
      <AnchorPane>
         <children>
            <VBox prefHeight="688.0" prefWidth="664.0">
               <children>
                  <HBox prefWidth="637.0" spacing="15.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#goToHistory" text="Rent History" />
                        <Button mnemonicParsing="false" onAction="#editAccount" text="Edit Account" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#signOut" text="Sign out" />
                     </children>
                  </HBox>
                  <VBox prefHeight="632.0" prefWidth="683.0" spacing="20.0">
                     <children>
                        <Label ellipsisString="" styleClass="header" text="Movies" />
                        <TilePane fx:id="moviesContainer" hgap="20.0" prefHeight="685.0" prefWidth="663.0" vgap="20.0" />
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
